<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <GridPane prefHeight="430.0" prefWidth="510.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints />
          <RowConstraints maxHeight="395.0" minHeight="10.0" prefHeight="374.0" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <HBox prefHeight="30.0" prefWidth="510.0">
               <children>
                  <ChoiceBox fx:id="difficultyBox" prefWidth="150.0" />
                  <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#startGame" text="Start" />
               </children>
            </HBox>
            <HBox prefHeight="400.0" prefWidth="510.0" GridPane.rowIndex="1">
               <children>
                  <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</GridPane>
